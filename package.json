{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:biome": "biome lint .",
    "lint:biome:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:performance": "jest --testPathPattern=performance",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "pre-commit": "biome check --write .",
    "taskmaster": "npx tsx taskmaster-ai/cli/taskmaster.ts",
    "taskmaster:interactive": "npx tsx taskmaster-ai/cli/taskmaster.ts --interactive",
    "taskmaster:config": "npx tsx taskmaster-ai/cli/taskmaster.ts --config",
    "taskmaster:setup": "powershell -ExecutionPolicy Bypass -File taskmaster-ai/scripts/setup.ps1",
    "taskmaster:component": "npx tsx taskmaster-ai/cli/taskmaster.ts -t component",
    "taskmaster:page": "npx tsx taskmaster-ai/cli/taskmaster.ts -t page",
    "taskmaster:hook": "npx tsx taskmaster-ai/cli/taskmaster.ts -t hook",
    "taskmaster:service": "npx tsx taskmaster-ai/cli/taskmaster.ts -t service"
  },
